{
  "manifest_version": 3,
  "name": "PhenoML Chat Extension",
  "version": "1.0",
  "description": "A browser extension for PhenoML chat functionality",
  "action": {
    "default_popup": "extension-popup.html",
    "default_title": "PhenoML Chat"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://pheno-test.app.pheno.ml/*",
    "http://127.0.0.1:8001/*"
  ]
}